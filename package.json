{
  "name": "Shatner",
  "description": "Renders an awseome pic of William Shatner matching the mood provided",
  "key": "shatner.pics",
  "links": {
    "homepage": "http://shatner.pics",
    "self": "http://shatner.pics"
  },
  "capabilities": {
    "webhook": [
      {
        "event": "room_message",
        "pattern": "^/shatner (\\w*?)$",
        "url":"http://shatner.pics/mood/{{$1}}.jpg",
        "name": "Shatner"
      }
    ]
  }
}
