{
  "name": "Shatner",
  "description": "Renders an awseome picture of William Shatner matching the mood provided",
  "key": "shatner.pics",
  "links": {
    "homepage": "http://shatner.pics",
    "self": "https://raw.githubusercontent.com/alphadevx/shatner-bot/master/package.json"
  },
  "capabilities": {
    "webhook": [
      {
        "event": "room_message",
        "pattern": "^/shatner (\\w*?)$",
        "url":"http://shatner.pics/mood/{{$1}}.jpg",
        "name": "Shatner",
        "authentication": "none"
      }
    ],
    "hipchatApiConsumer": {
      "scopes": [
        "send_notification"
      ]
    }
  }
}
